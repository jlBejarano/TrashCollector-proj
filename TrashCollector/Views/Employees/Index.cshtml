@model IEnumerable<TrashCollector.Models.Employee>

@{
    ViewData["Title"] = "Employee Home";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>
<html>
<head>
    <title>Map</title>
    <meta name="viewport" content="initial-scale=1.0" />
          <meta charset="utf-8" />
                <style>
                    #map{
                        height:100%;
                    }
                    html, body{
                        height:100%
                        margin:0;
                        padding:0;

                    }
                </style>
</head>
<body>
    <div id="map"</div>
    <script>
        var map;
        funciton initMap()
        {
            map = new google.maps.Map(document.getElementById('map'),
        }
        center: { lat: -43.0389, lng: -87.9065 },
        zoom: 7
        });
        }
        
    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyB74-RvWvZttGsfW1473z1WPyVPE3yPD4A&callback=initMap"
        async defer></script>
    
</body>
</html>

<h3>Welcome </h3>
    

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ZipCode)
            </th>
            
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ZipCode)
            </td>
            
            <td>
                <a asp-action="Edit" asp-route-id="@item.EmployeeId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.EmployeeId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.EmployeeId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
